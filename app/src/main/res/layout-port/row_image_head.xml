<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <CheckBox
        android:id="@+id/row_image_head_chk"
        android:layout_width="30dp"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/row_image_head_date"
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        android:text="2020.01.01 10:00"
        android:padding="5dp"
        />
    <TextView
        android:id="@+id/row_image_head_tranCode"
        android:layout_width="30dp"
        android:layout_height="wrap_content"
        android:text="13"
        android:padding="5dp"
        />
    <TextView
        android:id="@+id/row_image_head_moveNum"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:text="MoveNum"
        android:padding="5dp"
        />
    <TextView
        android:id="@+id/row_image_head_terminal"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:text="TERM999"
        android:padding="5dp"
        />
    <TextView
        android:id="@+id/row_image_head_userid"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:text="576586"
        android:padding="5dp"
        />
    <ImageView
        android:id="@+id/row_image_head_del"
        android:layout_width="30dp"
        android:layout_height="match_parent"
        android:src="@drawable/row_del"
        android:padding="4dp"
        />
</LinearLayout>